  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aiptx/aiptx-js.git"
  },
  "homepage": "https://aiptx.io",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aiptx/sdk": "file:../nodejs",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "commander": "^11.1.0",
    "conf": "^12.0.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  }
