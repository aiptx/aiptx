{
  "name": "@aiptx/cli",
  "version": "2.0.6",
  "description": "AIPTX CLI - AI-Powered Penetration Testing Command Line Interface",
  "bin": {
    "aiptx": "./dist/cli.js"
  },
  "main": "dist/cli.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/cli.ts --format cjs --clean --minify",
    "dev": "tsup src/cli.ts --format cjs --watch",
