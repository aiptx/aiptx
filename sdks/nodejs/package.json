{
  "name": "@aiptx/sdk",
  "version": "2.0.6",
  "description": "AIPTX - AI-Powered Penetration Testing SDK for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "aiptx",
    "security",
    "penetration-testing",
    "pentest",
    "vulnerability-scanner",
    "ai",
    "llm",
    "cybersecurity",
    "appsec",
    "owasp"
  ],
  "author": "Satyam Rastogi <satyam@aiptx.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aiptx/aiptx-js.git"
  },
  "homepage": "https://aiptx.io",
  "bugs": {
    "url": "https://github.com/aiptx/aiptx-js/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "eventsource": "^2.0.2",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/eventsource": "^1.1.15",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  }
}
